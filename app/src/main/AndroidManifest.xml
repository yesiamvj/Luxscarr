<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ulgebra.luxscarr">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BookingDetails" />
        <activity android:name=".BookingHistory" />
        <activity android:name=".BrowseCars" />
        <activity android:name=".CancelBooking" />
        <activity android:name=".ConfirmBooking" />
        <activity
            android:name=".SingleCarDetail"
            android:label="@string/title_activity_single_car_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AdminShownCars" />
        <activity android:name=".AdminHiddenCars" />
        <activity android:name=".AdminBookingHistory"></activity>
    </application>

</manifest>